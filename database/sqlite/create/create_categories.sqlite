CREATE TABLE IF NOT EXISTS /* %PREFIX% */categories (
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	title varchar(255),
	alias varchar(255),
	author varchar(255),
	description varchar(255),
	keywords varchar(255),
	robots int(1),
	language char(2),
	template varchar(255),
	sibling int(10),
	parent int(10),
	status int(1) DEFAULT 1,
	rank int(10),
	access varchar(255),
	date int(15)
);
