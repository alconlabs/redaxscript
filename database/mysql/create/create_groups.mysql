CREATE TABLE IF NOT EXISTS /* %PREFIX% */groups (
	id int(10) PRIMARY KEY AUTO_INCREMENT,
	name varchar(255) COLLATE utf8_unicode_ci,
	alias varchar(255) COLLATE utf8_unicode_ci,
	description varchar(255) COLLATE utf8_unicode_ci,
	categories varchar(255) COLLATE utf8_unicode_ci,
	articles varchar(255) COLLATE utf8_unicode_ci,
	extras varchar(255) COLLATE utf8_unicode_ci,
	comments varchar(255) COLLATE utf8_unicode_ci,
	groups varchar(255) COLLATE utf8_unicode_ci,
	users varchar(255) COLLATE utf8_unicode_ci,
	modules varchar(255) COLLATE utf8_unicode_ci,
	settings int(1),
	filter int(1) DEFAULT 1,
	status int(1) DEFAULT 1
)
ENGINE = MyISAM
DEFAULT CHARSET = utf8
COLLATE = utf8_unicode_ci
AUTO_INCREMENT = 1;
